{
  "name": "yaycmyk(dot)com",
  "description": "Source for yaycmyk.com",
  "version": "1.0.0",
  "private": true,
  "author": "Evan Scott <glitterbyte@gmail.com>",
  "devDependencies": {
    "autoprefixer-stylus": "^0.9.2",
    "budo": "^8.1.0",
    "parallelshell": "^3.0.0",
    "stylus": "^0.54.2"
  },
  "scripts": {
    "css:build": "stylus -c -u autoprefixer-stylus style.styl -o dist/css/style.css",
    "css:watch": "yarn css:build -s -- -w",
    "start": "parallelshell 'yarn css:watch' 'budo -l -o -H 0.0.0.0'",
    "release": "yarn css:build"
  }
}
